<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="328.0" prefWidth="532.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerStack">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textf" alignment="CENTER" prefHeight="45.0" prefWidth="181.0">
               <HBox.margin>
                  <Insets left="100.0" right="10.0" top="30.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#pushAction" onDragDetected="#pushAction" prefHeight="43.0" prefWidth="63.0" style="-fx-background-color: POWDERBLUE;" text="Push" textOverrun="CLIP">
               <HBox.margin>
                  <Insets right="10.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font name="DejaVu Sans Bold" size="13.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#popAction" onDragDetected="#popAction" prefHeight="42.0" prefWidth="62.0" style="-fx-background-color: POWDERBLUE;" text="Pop">
               <HBox.margin>
                  <Insets top="30.0" />
               </HBox.margin>
               <font>
                  <Font name="DejaVu Sans Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </top>
   <center>
      <HBox fx:id="hbox" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="50.0" left="40.0" right="30.0" top="50.0" />
         </BorderPane.margin>
      </HBox>
   </center>
</BorderPane>
