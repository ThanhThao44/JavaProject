<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="496.0" prefWidth="522.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerBtree">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textf" prefHeight="40.0" prefWidth="103.0" promptText="Enter value">
               <HBox.margin>
                  <Insets left="40.0" right="20.0" top="30.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#insertAction" onDragDetected="#insertAction" prefHeight="40.0" prefWidth="62.0" style="-fx-background-color: POWDERBLUE;" text="Insert">
               <font>
                  <Font name="DejaVu Sans Bold" size="13.0" />
               </font>
               <HBox.margin>
                  <Insets top="30.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteAction" onDragDetected="#deleteAction" prefHeight="41.0" prefWidth="69.0" style="-fx-background-color: POWDERBLUE;" text="Delete">
               <HBox.margin>
                  <Insets left="20.0" top="30.0" />
               </HBox.margin>
               <font>
                  <Font name="DejaVu Sans Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </top>
   <center>
      <AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
